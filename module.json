{
  "id": "hm-cpn-earlsprogress",
  "title": "Earls Progress",
  "version": "1.2.1",
  "bugs": "https://github.com/toastygm/hm-cpn-earlsprogress/issues",
  "license": "LICENSE",
  "readme": "README.md",
  "authors": [
    {
      "name": "Toasty",
      "discord": "toasty#8538",
      "flags": {}
    }
  ],
  "compatibility": {
    "minimum": "11",
    "verified": "12.331"
  },
  "styles": [
    "css/ep.css"
  ],
  "relationships": {
    "requires": [
      {
        "id": "scene-packer",
        "type": "module",
        "manifest": "https://raw.githubusercontent.com/League-of-Foundry-Developers/scene-packer/master/module.json",
        "compatibility": {}
      }
    ]
  },
  "scripts": [
    "scripts/init.js"
  ],
  "packFolders": [
    {
      "name": "Earl's Progress",
      "sorting": "m",
      "packs": [
        "scenes",
        "journals",
	"rolltables"
      ]
    }
  ],

  "packs": [
    {
      "label": "Scenes",
      "name": "scenes",
      "path": "packs/scenes",
      "type": "Scene",
      "flags": {},
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "label": "Journals",
      "name": "journals",
      "path": "packs/journals",
      "type": "JournalEntry",
      "flags": {},
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    },
    {
      "label": "RollTables",
      "name": "rolltables",
      "path": "packs/rolltables",
      "type": "RollTable",
      "flags": {},
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    }
  ],
  "url": "https://github.com/toastygm/hm-cpn-earlsprogress/",
  "manifest": "https://github.com/toastygm/hm-cpn-earlsprogress/releases/latest/download/module.json",
  "download": "https://github.com/toastygm/hm-cpn-earlsprogress/releases/download/v1.2.1/module.zip",
  "flags": {
    "allowBugReporter": true
  }
}
